*
*::after 
*::before {
    box-sizing border-box
}
a {
    text-decoration none
}

body {
    max-width: 1200px;
    margin: auto!important;
}

.nexmoe-bg {
    opacity: 0.2;
    background-size: cover;
    filter: blur(30px);
    height: 100vh;
    width: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: -1;
}

.mdui-drawer {
    z-index: 500;     
}

#nexmoe-header{
    margin-left: -240px;
    .mdui-drawer {
        left: unset;
        scrollbar-width: none;
    }
    .mdui-drawer::-webkit-scrollbar {
        display: none;
    }
    .nexmoe-avatar
    .nexmoe-avatar img {
        width: 100%;
        font-size:0 0;
    }
    .nexmoe-avatar {
        margin: 45px;
        z-index: 4;
        position: relative;
        font-size: 0;
        width: calc(100% - 90px);
        overflow: hidden;
        border-radius: 100%;
        box-shadow: 0 0.3rem 2rem rgba(161,177,204,.6);
        a {
            padding-bottom: 100%;
            display: block;
            height: 0;
        } 
    }

    .nexmoe-count {
        padding: 0 30px;
        margin-bottom: 30px;
        display: -webkit-box;
        display: -ms-flexbox;
        display: flex;
        div {
            -webkit-box-flex: 1;
            -ms-flex: 1;
            flex: 1;
            text-align: center;
            color: #a3a8ae;
            span {
                color: #363636;
                display: block;
            }
        }
    }
    .nexmoe-social {
        padding: 5px;
        a {
            width: 36px;
            height: 36px;
            line-height: 36px;
            margin: 5px;
            border-radius: 100%;
            display: inline-block;
            text-align: center;
            color: #606266;
        }
    }
    .nexmoe-list {
        padding: 8px 20px;
        .nexmoe-list-item  {
            padding: 0 36px;
            color: #9ca2a8;
            transition: all .35s;
            border-radius: 10px;
            margin-bottom: 10px;
            text-align: center;
        }
        .nexmoe-list-item.active,
        .nexmoe-list-item.active i {
            color: #fff;
        }
        
        .nexmoe-list-item.active {
            opacity: .9;
            background: #ff4e6a;
            box-shadow: 0 2px 12px #ff4e6a;
        }
        .nexmoe-list-item i {
            font-size: 22px;
            color: #9ca2a8;
        }
        .nexmoe-list-item .mdui-list-item-content {
            margin-left: 0px;
            transition: all .35s;
        }
    }
    .nexmoe-widget-wrap {
            box-shadow: 0 0 1rem rgba(161,177,204,0.4);
            background-color: #fff;
            margin: 20px;
            border-radius: 10px;
            overflow: hidden;
            white-space: normal;
            .nexmoe-widget-title {
                font-size: 1.2em;
                font-weight: normal;
                padding: 12px 10px 10px;
                border-bottom: 1px solid #eee;
                margin: 0;
            }
            .nexmoe-widget {
                ul {
                    list-style-type: none;
                    padding: 0;
                    margin: 0;
                    li {
                        overflow: hidden;
                        text-overflow: ellipsis;
                        white-space: nowrap;
                        border-bottom: 1px solid #eee;
                        position: relative;
                        a {
                            padding: 14px 10px;
                            display: block;
                            color: #606266;
                        }
                    }
                    li:last-child {
                        border-bottom: none;
                    }
                }
                .category-list-count {
                    background-color: rgba(255, 78, 106, .8);
                    display: inline-block;
                    width: 26px;
                    height: 26px;
                    line-height: 26px;
                    text-align: center;
                    border-radius: 100%;
                    color: #fff;
                    position: absolute;
                    right: 10px;
                    top: 10px;
                }
            }
            .tagcloud {
                padding: 10px;
                padding-bottom: 5px;
                a {
                    border-radius: 10px;
                    padding:5px 10px;
                    color:#fff;
                    font-size:12px!important;
                    display:inline-block;
                    margin-bottom:5px
                }
                a:before {
                    content: "# ";
                }
                a:nth-child(8n+1) {
                    background-color:rgba(255,78,106,.15);
                    color: rgba(255,78,106,.8);
                }
                a:nth-child(8n+2) {
                    background-color:rgba(255, 170, 115,.15);
                    color: rgb(255, 170, 115);
                }
                a:nth-child(8n+3) {
                    background-color:rgba(254, 212, 102, .15);
                    color: rgb(254, 212, 102);
                }
                a:nth-child(8n+4) {
                    background-color:rgba(60, 220, 130, .15)
                    color: rgb(60, 220, 130);
                }
                a:nth-child(8n+5) {
                    background-color:rgba(100, 220, 240, .15)
                    color: rgb(100, 220, 240);
                }
                a :nth-child(8n+6) {
                    background-color:rgba(100, 185, 255, .15)
                    color: rgb(100, 185, 255);
                }
                a:nth-child(8n+7) {
                    background-color:rgba(180, 180, 255, .15)
                    color: rgb(180, 180, 255);
                }
                a:nth-child(8n+8) {
                    background-color:rgba(244, 167, 185, .15)
                    color: rgb(244, 167, 185);
                }
            }
        }
}

@media screen and (max-width: 1024px) {
    #nexmoe-header{
        margin-left: 0;
        .mdui-drawer {
            left: 0;
        }
    }
}

#nexmoe-content {
    position: relative;
    .nexmoe-tab {
        margin: -20px;
        margin-bottom: 0;
        line-height: 64px;
        min-height: 20px;
        a {
            margin-left: 20px;
            cursor: pointer;
            display: none;
        }
    }
    .nexmoe-primary {
        box-shadow: 0 0 1rem rgba(161,177,204,0.4);
        background-color: #fff;
        float: left;
        width: 100%;
        padding: 20px;
        position: absolute;
        min-height: 100vh;
        padding-bottom: 58px;
        .nexmoe-page-nav {
            padding: 0;
            padding-bottom: 20px;
            margin: 0;
            list-style: none;
            text-align: center;
            color: #DDD;
            .page-number, .extend {
                transition: all .35s;
                color: #999;
                display: block;
                border-radius: 10px;
                width: 35px;
                height: 35px;
                line-height: 35px;
                display: inline-block;
                margin: 5px 8px;
                background-color: #f5f6f5;
            }
            .current {
                color: #fff;
                background: #ff4e6a;
                opacity: .8;
            }
            .prev, .next {
                width: 50px;
            }
        }
        .nexmoe-copyright {
            margin: 0 -20px;
            width: 100%;
            padding: 20px;
            margin-bottom: -20px;
            text-align: center;
            border-top: 1px solid #eee;
            background-color: #f5f6f5;
            color: #777;
            position: absolute;
            bottom: 0;
            a {
                color: rgba(255,78,106,.8);
            }
        }
    }
    .nexmoe-posts {
        position relative
        .nexmoe-post {
            width: 100%;
            margin-bottom: 15px;
            article p:first-child {
                margin-top: 0;
            }
            article p:last-child {
                margin-bottom: 0;
            }
        }
    }
    .nexmoe-post {
            .nexmoe-post-cover {
                width: 100%;
                position: relative;
                overflow: hidden;
                border-radius: 10px;
                margin-bottom: 10px;
                max-height: 500px;
                min-height: 65px;
                background-color: #eee;
                img {
                    width: 100%;
                        display: block;
                }
                h1 {
                    position: absolute;
                    bottom: 0;
                    padding: 15px;
                    color: #fff;
                    font-size: 2em;
                    width: 100%;
                    box-sizing: border-box;
                    text-transform: none;
                    z-index: 1;
                    margin: 0;
                    font-weight: normal;
                }
                h1:after {
                    background-image: -webkit-linear-gradient(to top,rgba(10,17,25,.2) 10%,rgba(10,17,25,0) 100%);
                    background-image: linear-gradient(to top,rgba(10,17,25,.2) 10%,rgba(10,17,25,0) 100%);
                    -moz-pointer-events: none;
                    -webkit-pointer-events: none;
                    -ms-pointer-events: none;
                    pointer-events: none;
                    background-size: cover;
                    content: '';
                    display: block;
                    height: 100%;
                    left: 0;
                    position: absolute;
                    bottom: 0;
                    width: 100%;
                    z-index: -1;
                }
            }
            .nexmoe-post-meta {
                margin-bottom: -10px;
                span {
                    border-radius: 10px;
                    padding:5px 10px;
                    color:#fff;
                    font-size:14px;
                    display:inline-block;
                    margin-bottom:5px;
                    margin-right:3px;
                    .nexmoefont {
                        font-size: 14px;
                    }
                }
                span a {
                    color:#fff;
                }
                span:nth-child(8n+1) {
                    background-color:#ff4e6a
                }
                span:nth-child(8n+2) {
                    background-color:#ffaa73
                }
                span:nth-child(8n+3) {
                    background-color:#fed466
                }
                span:nth-child(8n+4) {
                    background-color:#3cdc82
                }
                span:nth-child(8n+5) {
                    background-color:#64dcf0
                }
                span:nth-child(8n+6) {
                    background-color:#64b9ff
                }
                span:nth-child(8n+7) {
                    background-color:#b4b4ff
                }
                span:nth-child(8n+8) {
                    background-color:#f4a7b9
                }
            }
            article {
                padding: 10px 0;
                margin-bottom: 30px;
            }
        }
    .nexmoe-comment {
        background-color: #f5f6f5;
        padding: 20px;
        margin: -20px;
    }

}
@media screen and (max-width: 1023px) {
    .nexmoe-tab {
        a {
            display: block!important;
        }
    }
}
#nexmoe-content::after{
    clear: both;
    display: table;
    content: ' ';
}
@media screen and (max-width:1024px) {
    #nexmoe-content {
        .nexmoe-primary {
            width: 100%;
            position: relative;
            min-height: 100vh;
        }
        .nexmoe-secondary {
            display: none;
        }
    }
}
@media screen and (min-width:1024px) {
    body {
        padding-left: 80px;
    }
    #nexmoe-header .mdui-drawer-close {
        -webkit-transform: translateX(-160px);
        transform: translateX(-160px);
        .nexmoe-avatar {
            padding: 16px;
            width: 80px;
            margin-left: 160px;
            margin-top: 8px;
        }
        .nexmoe-count {
            padding: 16px;
            width: 80px;
            margin-left: 160px;
            display: block;
        }
        .nexmoe-count div {
            display: block;
            margin-bottom: 10px;
        }
        .nexmoe-count div:last-child {
            margin-bottom: 0;
        }
        .mdui-list {
            margin-left: 160px;    
        }
        .nexmoe-list-item  {
            padding: 0 17px;
            color: #9ca2a8;
        }
        .mdui-list-item-icon~.mdui-list-item-content {
            margin-left: 32px;
        }
        .nexmoe-list {
            padding: 8px 10px;
        }
    }
}
article {
    color:#444;
    font-family:microsoft yahei
}
article p {
    line-height:24px
}
article code {
    background-color: rgba(27,31,35,.05);
    border-radius: 6px;
    font-size: 85%;
    padding: .2em .4em;
    border: 1px solid #d7dae2;
}
article code::-webkit-scrollbar {
    width:10px;
    height:10px
}
article code::-webkit-scrollbar-thumb {
    border-radius:6px;
    background-color:#888
}
article pre {
    border: 1px solid #ddd;
    border-radius: 6px;
    overflow hidden
    max-height:600px;
    -webkit-box-sizing:border-box;
    box-sizing:border-box;
    margin:20px 0;
    position: relative;
}
article pre::before {
    content: "";
    background: #282c34;
    height: 32px;
    margin-bottom: 0;
    display: block;
}
article pre::after {
    content: " ";
    position: absolute;
    border-radius: 50%;
    background: #ff5f56;
    width: 12px;
    height: 12px;
    top: 0;
    left: 12px;
    margin-top: 12px;
    -webkit-box-shadow: 20px 0 #ffbd2e, 40px 0 #27c93f;
    box-shadow: 20px 0 #ffbd2e, 40px 0 #27c93f;
}
article pre code {
    position: relative;
    background: #282c34!important;
    padding: 0 5px;
    display: inline-block;
    padding-left: 45px!important;
    border: none;
    border-radius: 0;
    font-size: 100%;
}
